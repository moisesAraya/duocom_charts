{
  "expo": {
    "name": "charts_duocom",
    "slug": "charts_duocom",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/images/img_.png",
    "scheme": "chartsduocom",
    "userInterfaceStyle": "automatic",
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#FFFFFF",
        "foregroundImage": "./assets/images/img_.png"
      },
      "edgeToEdgeEnabled": false,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "android.permission.INTERNET",
        "android.permission.ACCESS_NETWORK_STATE"
      ],
      "usesCleartextTraffic": true,
      "package": "com.duocommovil.charts_duocom"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/img.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": false
    },
    "owner": "moisesarayaa",
    "extra": {
      "router": {},
      "eas": {
        "projectId": "f5a69da6-4550-4977-a2d6-310614b4abc1"
      },
      "apiUrl": "https://duocom.dyndns.org/charts",
      "apiKey": ""
    }
  }
}

